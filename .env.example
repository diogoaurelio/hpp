# MinIO Configuration for local testing
AWS_ACCESS_KEY_ID=admin
AWS_SECRET_ACCESS_KEY=admin123
AWS_ENDPOINT=http://localhost:9000
AWS_REGION=eu-central-1

# S3 API Configuration
INTERFACE=127.0.0.1
PORT=8989

# IAM API Configuration
IAM_API_URL=http://127.0.0.1:8988

# IAM S3 Storage Configuration (for persistent IAM data)
# If these are set, IAM service will use S3 storage instead of in-memory
S3_ENDPOINT=http://localhost:9000
S3_REGION=eu-central-1
S3_ACCESS_KEY=admin
S3_SECRET_KEY=admin123
IAM_S3_BUCKET=iam-data

# Bedrock API Configuration
BEDROCK_PORT=8990

# Bedrock S3 Storage Configuration (for persistent vector data)
BEDROCK_S3_BUCKET=bedrock-data

# HuggingFace Embeddings Configuration
USE_HUGGINGFACE_EMBEDDINGS=true
HF_MODELS_TO_LOAD=sentence-transformers/all-MiniLM-L6-v2,sentence-transformers/all-mpnet-base-v2
HF_CACHE_DIR=./hf_cache